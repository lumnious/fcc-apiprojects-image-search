{
{{#each results}}
	<p>{{#each this}}
		"{{@key}}" : 
			{{#ifObject this}}
				{ {{#each this}}
					"{{@key}}" : "{{this}}" {{#unless @last}}, {{/unless}}
				{{/each}} }{{#unless @last}}, {{/unless}}
			{{else}}
				{{#iflink this}}
				"<a href="{{this}}">{{this}}</a>" {{#unless @last}}, {{/unless}}
					{{else}}
						"{{this}}" {{#unless @last}}, {{/unless}}
					{{/iflink}}
			{{/ifObject}}
	{{/each}}{{#unless @last}},{{/unless}}</p>
{{/each}}
}